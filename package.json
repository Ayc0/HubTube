{
  "name": "toddy_music",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.15.3",
    "express": "^4.15.2",
    "lodash": "^4.17.4",
    "material-ui": "^0.17.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-swipeable-views": "^0.11.1",
    "react-youtube": "^7.3.0",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "socket.io-react": "^1.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "ghooks": "^2.0.0",
    "nodemon": "^1.11.0",
    "react-scripts": "0.9.5",
    "validate-commit-msg": "^2.11.1"
  },
  "scripts": {
    "server": "nodemon server.js --exec babel-node --presets es2015",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "lint": "eslint --ext .js ./src/",
    "eject": "react-scripts eject"
  },
  "config": {
    "ghooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feature",
        "config",
        "fix",
        "package",
        "docs",
        "style",
        "refactor",
        "test",
        "revert"
      ],
      "scope": {
        "required": true,
        "allowed": "*",
        "validate": true,
        "multiple": true
      },
      "warnOnFail": false,
      "autoFix": false,
      "maxSubjectLength": 1000
    }
  }
}
